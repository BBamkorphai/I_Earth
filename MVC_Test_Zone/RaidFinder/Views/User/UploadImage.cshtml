@using RaidFinder.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Image

<form asp-action="UploadImage" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="ImageData" class="control-label"></label>
        <input type="file" name="file" id="image" class="form-control-file" />
    </div>
    <b>Id </b><input type="text" asp-for="OwnerId" /><br />
    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-primary" />
    </div>
</form>